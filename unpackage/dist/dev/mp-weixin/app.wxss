@charset "utf-8";
/*每个页面公共css */
/*CSS reset*/
/* 全局重置 */
@font-face {
	font-family: 'Source Han Sans';
	src: url('https://static.dstp.com.cn/fonts/SourceHanSansCN-Regular.otf') format('opentype');
}
@font-face {
	font-family: 'Source Han Sans Light';
	src: url('https://static.dstp.com.cn/fonts/SourceHanSansCN-Light.otf') format('opentype');
}
@font-face {
	font-family: 'Source Han Sans Medium';
	src: url('https://static.dstp.com.cn/fonts/SourceHanSansCN-Medium.otf') format('opentype');
}
html {
	-webkit-text-size-adjust: none;
	font-size: 100rpx;
	-webkit-user-select: none;
}
html,
body {
	margin: 0;
	padding: 0;
	height: 100%;
}
body {
	font-size: 20rpx;
	font-family: 'Source Han Sans', sans-serif;
	font-weight: normal;
	font-style: normal;
	/* 使用该字体 */
	color: #2b2b2b;
}
/*用到的元素盒模型重置*/
div,
img,
p,
h1,
h2,
h3,
input,
select,
button,
table,
th,
td,
form,
ul,
ol,
li,
em,
strong,
a,
span,
i {
	margin: 0;
	padding: 0;
	border: none;
	outline: none;
}
view,
text,
image,
button,
input,
textarea,
checkbox-group,
label,
checkbox,::before,::after {
	box-sizing: border-box;
}
/* * {
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-webkit-tap-highlight-color: transparent;
}

*:before,
*:after {
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-webkit-tap-highlight-color: transparent;
} */
/*列表重置*/
li {
	list-style: none;
}
/*表格重置*/
table {
	border-collapse: collapse;
	border-spacing: 0;
}
caption,
th,
td {
	text-align: left;
}
/*表单重置*/
select {
	-webkit-appearance: none;
	background-color: transparent;
	border-radius: 0;
	border: none;
}
input {
	background-color: transparent;
	border-radius: 0;
	-webkit-border-radius: 0;
	line-height: normal;
	-webkit-appearance: none;
}
input[type='button'],
input[type='submit'] {
	-webkit-appearance: none;
}
input::-webkit-input-placeholder {
	color: #999;
}
input[type='file'] {
	width: 100%;
	height: 100%;
	opacity: 0;
}
textarea {
	resize: none;
	border: 0;
}
input,
button,
textarea {
	font-family: 'Source Han Sans', sans-serif;
	font-weight: normal;
	font-style: normal;
}
button {
	background-color: transparent;
	-webkit-appearance: none;
}
/*文本重置*/
h1,
h2,
h3,
h4,
h5,
h6,
del,
caption,
th,
td,
strong,
em,
i {
	font-size: 100%;
	font-style: normal;
	font-weight: normal;
}
del,
ins {
	text-decoration: none;
}
q::before,
q::after {
	content: '';
}
/*链接重置*/
a {
	text-decoration: none;
	color: #333;
}
a:active {
	outline: 0;
}
/*去掉图片4rpx下边距*/
img {
	display: block;
	width: 100%;
	-webkit-touch-callout: none;
}

